{
    "dependencies": {
        "lodash-es": "4.17.22",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "simpledotcss": "2.3.7"
    },
    "description": "Browser extension with additional GitHub functionality",
    "devDependencies": {
        "@stylistic/eslint-plugin": "5.6.1",
        "@types/lodash-es": "4.17.12",
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.3",
        "@wxt-dev/auto-icons": "1.1.0",
        "@wxt-dev/module-react": "1.1.5",
        "eslint": "9.39.2",
        "eslint-plugin-collation": "1.5.1",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-perfectionist": "5.1.0",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "7.0.1",
        "prettier": "3.7.4",
        "typescript": "5.9.3",
        "typescript-eslint": "8.50.1",
        "wxt": "0.20.13"
    },
    "name": "extended-github",
    "packageManager": "npm@11.6.0",
    "private": true,
    "scripts": {
        "build": "wxt build",
        "build:firefox": "wxt build -b firefox",
        "compile": "tsc --noEmit",
        "dev": "wxt",
        "dev:firefox": "wxt -b firefox",
        "format": "prettier --check \"**/*.{json,yml,js,ts,tsx,md}\"",
        "format:fix": "prettier --write \"**/*.{json,yml,js,ts,tsx,md}\"",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "postinstall": "wxt prepare",
        "test": "echo 'TODO'",
        "zip": "wxt zip",
        "zip:firefox": "wxt zip -b firefox"
    },
    "type": "module",
    "version": "0.0.0"
}
